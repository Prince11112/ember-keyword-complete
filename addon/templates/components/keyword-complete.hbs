{{yield (hash
  tooltip=(component 'keyword-complete-tooltip' id=(concat elementId '-tooltip') position=tooltipCoords)
  item=(component 'keyword-complete-tooltip-item' classNames=keyItemClassName keyItemComponent=keyItemComponent selected-item=(action 'clickedSelection'))
  suggestions=suggestions
  selectionIdx=selectionIdx
  filterQuery=filterQuery
  visible=tooltipVisible
  keyItemComponent=keyItemComponent
  showTypingState=showTypingState
  isLoadingSuggestions=isLoadingSuggestions
  select=(action 'clickedSelection')
  hover=(action 'hoverSelection')
  refresh=(action 'refreshSuggestions')
  setCaretPosition=setCaretPositionAction
  onClose=onClose
  errors=errors
)}}
